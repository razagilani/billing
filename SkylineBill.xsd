<xs:schema xmlns:sb="SkylineBill" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="SkylineBill" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="skylinebill">
		<xs:annotation>
			<xs:documentation>Given a SNUB and rate schedule, all the consumption data necessary to compute SNOB</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="measuredusage" maxOccurs="2">
					<xs:annotation>
						<xs:documentation>Usage per service</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="meter" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="identifier"/>
										<xs:element name="register" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="identifier"/>
													<xs:element name="units"/>
													<xs:element name="description"/>
													<xs:element name="total"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="service">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="gas"/>
									<xs:enumeration value="electric"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
